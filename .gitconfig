[user]
	name = Jan Koeppen
	email = jan@yaneq.com
[core]
	excludesfile = /Users/jan/.gitignore_global
	autocrlf = input
	editor = vim
	compression = -1
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[push]
	default = matching
[cola]
	spellcheck = false
	fontdiff = Ubuntu Mono for Powerline,18,-1,5,50,0,0,0,0,0
[merge]
	conflictstyle = diff3
